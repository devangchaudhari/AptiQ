{
    "version": 2,
    "builds": [
        {
            "src": "*server.js", "use": "@vercel/node"
        }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
     
    ],
    "rewrites": [
      { "source": "/api/submit-answers", "destination": "/api/submit-answers" },
      { "source": "/certify", "destination": "/certify" },
      { "source": "/questions/:category", "destination": "/questions/:category" },
      { "source": "/(.*)", "destination": "/index.html" }
    ]
 
  }

  
  